{
  "require": {
    "adigital/cookie-consent-banner": "1.2.7",
    "aelvan/mailchimp-subscribe": "3.0.3",
    "craftcms/cms": "3.6.9",
    "craftcms/contact-form": "2.2.7",
    "craftcms/redactor": "2.8.5",
    "enupal/stripe": "3.3.2",
    "ether/seo": "3.6.7",
    "hybridinteractive/craft-position-fieldtype": "1.0.17",
    "jmx2/pic-puller": "3.0.15",
    "mikestecker/craft-videoembedder": "1.1.4",
    "misterbk/mix": "1.5.2",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
